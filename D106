N = input()
if len(N) >= 2:
    if N[-1] == '1' and N[-2] != '1': 
       print(N + 'st')
    elif N[-1] == '2' and N[-2] != '1':
       print(N + 'nd')
    elif N[-1] == '3'and N[-2] != '1':
       print(N + 'rd')
    else:
       print(N + 'th')
elif N == '1':
    print(N + 'st')
elif N == '2':
    print(N + 'nd')    
elif N == '3':
    print(N + 'rd')    
else:
    print(N + 'th')
